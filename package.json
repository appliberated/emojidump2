{
    "name": "emojidump",
    "version": "1.0.0",
    "description": "",
    "keywords": [],
    "author": "PWALab",
    "license": "MIT",
    "eslintConfig": {
        "env": {
            "browser": true,
            "es6": true
        },
        "extends": "eslint:all",
        "parserOptions": {
            "sourceType": "module"
        },
        "rules": {
            "array-element-newline": "off",
            "arrow-parens": ["error", "as-needed"],
            "capitalized-comments": ["error", "always", { "ignorePattern": "emojidump" }],
            "curly": ["error", "multi-line"],
            "dot-location": "off",
            "func-style": "off",
            "id-length": ["error", { "exceptions": ["i", "j"] }],
            "linebreak-style": ["error", "windows"],
            "max-len": ["error", 120],
            "multiline-ternary": ["error", "always-multiline"],
            "no-console": "off",
            "no-magic-numbers": ["error", { "ignore": [0, 1, 2] }],
            "no-multi-assign": "off",
            "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }],
            "no-ternary": "off",
            "object-curly-spacing": "off",
            "object-property-newline": ["error", { "allowAllPropertiesOnSameLine": true }],
            "one-var": "off",
            "padded-blocks": "off",
            "quote-props": ["error", "as-needed"],
            "sort-keys": "off",
            "space-before-function-paren": "off",
            "wrap-regex": "off"
        }
    }
}